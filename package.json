{
    "name": "mini-app",
    "version": "0.0.0",
    "eslintConfig": {
        "env": {
            "browser": true,
            "node": true
        }
    },
    "scripts": {
        "start": "cross-env PORT=10888 HTTPS=true react-scripts start",
        "build": "react-scripts build",
        "predeploy": "npm run build",
